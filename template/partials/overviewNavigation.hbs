{{#if overviewNavigation.menu.[1] }}
<nav>
    <ul>
        {{#each overviewNavigation.menu}}
        {{#if (eq link ../currentLink) }}<li class="active">{{/if}}

            {{#if (ne link ../currentLink) }}
        <li>{{/if}}
            <time datetime="{{date}}">
                <span class="day">{{dateFormat date format="dd"}}</span>
                <span class="month">{{dateFormat date format="MM"}}</span>
                <span class="year">{{dateFormat date format="yyyy"}}</span></time>

            <a href="{{ link }}">{{ name }}</a>
        </li>
        {{/each}}
    </ul>
</nav>
{{/if}}