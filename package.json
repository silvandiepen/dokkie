{
	"name": "dokkie",
	"version": "0.1.1",
	"description": "Create good looking documentation from your Readme",
	"main": "dist/index.js",
	"bin": "dist/index.js",
	"scripts": {
		"test": "jest",
		"build": "tsc",
		"build:dev": "tsc src -w",
		"serve": "serve -l 5555 dokkie",
		"dev:debug": "npm run build && node dist/index.js --debug=true --skip=favicons",
		"dev:docs": "npm run build && node dist/index.js --config=docs.config.json",
		"dev:blog": "npm run build && node dist/index.js --config=blog.config.json --skip=favicons --type=blog --input=posts",
		"pre": "npm run build && npm run test && npm run dev:docs",
		"precommit": "npm run pre",
		"prepublishOnly": "npm run pre"
	},
	"keywords": [
		"Documentation",
		"Create",
		"Markdown"
	],
	"author": "Sil van Diepen",
	"license": "MIT",
	"devDependencies": {
		"@types/favicons": "^5.5.0",
		"@types/jest": "^26.0.4",
		"@types/markdown-it": "^10.0.1",
		"@types/node-fetch": "^2.5.7",
		"@types/rimraf": "^3.0.0",
		"git-pre-commit": "^2.1.4",
		"jest": "^26.1.0",
		"serve": "^11.3.2",
		"ts-jest": "^26.1.2",
		"typescript": "^3.9.6"
	},
	"dependencies": {
		"@types/markdown-it-anchor": "^4.0.4",
		"canvas": "^2.6.1",
		"canvas-to-buffer": "^1.1.0",
		"cli-block": "^1.1.7",
		"date-fns": "^2.14.0",
		"favicons": "^6.1.0",
		"handlebars": "^4.7.6",
		"iconator": "^0.1.12",
		"markdown-it": "^11.0.0",
		"markdown-it-alert": "^0.1.1",
		"markdown-it-anchor": "^5.3.0",
		"markdown-it-emoji": "^1.4.0",
		"markdown-it-html5-media": "^0.6.0",
		"markdown-it-meta": "0.0.1",
		"markdown-it-prism": "^2.1.0",
		"markdown-it-task-lists": "^2.1.1",
		"ncp": "^2.0.0",
		"node-fetch": "^2.6.0",
		"prettier": "^2.0.5",
		"purgecss": "^2.3.0",
		"rimraf": "^3.0.2",
		"yargs": "^15.4.1"
	},
	"engineStrict": true,
	"engines": {
		"node": ">=12.0.0"
	}
}
