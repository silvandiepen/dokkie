{
	"name": "dokkie",
	"version": "0.0.41",
	"description": "Create good looking documentation from your Readme",
	"main": "dist/index.js",
	"bin": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"dokkie": "npm run build && node dist/index.js --layout=advanced ",
		"dev": "tsc src -w",
		"pre": "npm run build && npm run dokkie",
		"precommit": "npm run pre",
		"prepublishOnly": "npm run pre"
	},
	"keywords": [
		"Documentation",
		"Create",
		"Markdown"
	],
	"author": "Sil van Diepen",
	"license": "MIT",
	"devDependencies": {
		"@types/favicons": "^5.5.0",
		"@types/jest": "^25.2.3",
		"@types/markdown-it": "^10.0.1",
		"@types/rimraf": "^3.0.0",
		"git-pre-commit": "^2.1.4",
		"jest": "^26.0.1",
		"ts-jest": "^26.0.0",
		"typescript": "^3.9.3"
	},
	"dependencies": {
		"@types/markdown-it-anchor": "^4.0.4",
		"canvas": "^2.6.1",
		"canvas-to-buffer": "^1.1.0",
		"cli-block": "^1.1.7",
		"date-fns": "^2.14.0",
		"favicons": "^6.1.0",
		"handlebars": "^4.7.6",
		"markdown-it": "^11.0.0",
		"markdown-it-anchor": "^5.3.0",
		"markdown-it-emoji": "^1.4.0",
		"markdown-it-html5-media": "^0.6.0",
		"markdown-it-meta": "0.0.1",
		"markdown-it-prism": "^2.0.5",
		"markdown-it-task-lists": "^2.1.1",
		"ncp": "^2.0.0",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2",
		"yargs": "^15.3.1"
	},
	"dokkie": {
		"layout": "advanced",
		"strip": "pages"
	}
}
