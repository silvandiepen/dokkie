{
	"name": "dokkie",
	"version": "0.0.18",
	"description": "Create good looking documentation from your Readme",
	"main": "dist/index.js",
	"bin": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"dokkie": "npm run build && node dist/index.js --strip=pages",
		"dev": "tsc -w",
		"prepublishOnly": "npm run build && npm run dokkie"
	},
	"keywords": [
		"Documentation",
		"Create",
		"Markdown"
	],
	"author": "Sil van Diepen",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^25.2.3",
		"@types/markdown-it": "^10.0.1",
		"@types/rimraf": "^3.0.0",
		"jest": "^26.0.1",
		"ts-jest": "^26.0.0",
		"typescript": "^3.9.3"
	},
	"dependencies": {
		"cli-block": "^1.1.6",
		"handlebars": "^4.7.6",
		"markdown-it": "^11.0.0",
		"markdown-it-meta": "0.0.1",
		"markdown-it-prism": "^2.0.5",
		"ncp": "^2.0.0",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2",
		"yargs": "^15.3.1"
	},
	"dokkie": {
		"layout": "advanced",
		"strip": "pages"
	}
}
